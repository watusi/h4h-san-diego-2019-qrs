<div class="container-fluid">

  <div class="row page-title">
    <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
      <a href="<%= url_for :action => :index %>" class="btn btn-default btn-sm pull-left">Cancel</a>
    </div>
    <div class="col-xs-8 col-sm-8 col-md-8 col-lg-8">
      <h3 class="text-center">New <%= @capturedqr.title %></h3>
    </div>
    <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
    </div>
  </div>

  <form method="POST" action="<%= url_for :action => :create %>">
    <input type="hidden" name="id" value="<%= @capturedqr.object %>"/>

    
        <div class="form-group">
          <label for="capturedqr[content]">Content</label>
          <input type="text" id="capturedqr[content]" name="capturedqr[content]" <%=
          placeholder("Content") %> class="form-control" />
        </div>
    
        <div class="form-group">
          <label for="capturedqr[createdAt]">Createdat</label>
          <input type="text" id="capturedqr[createdAt]" name="capturedqr[createdAt]" <%=
          placeholder("Createdat") %> class="form-control" />
        </div>
    

    <button type="submit" class="btn btn-success btn-block">Create</button>
  </form>

</div>